@prefix owl:     <http://www.w3.org/2002/07/owl#> .
@prefix this:    <http://mex.aksw.org/examples/001/> .
@prefix xsd:     <http://www.w3.org/2001/XMLSchema#> .
@prefix rdfs:    <http://www.w3.org/2000/01/rdf-schema#> .
@prefix dct:     <http://purl.org/dc/terms/> .
@prefix mexcore:  <http://mex.aksw.org/mex-core#> .
@prefix doap:    <http://usefulinc.com/ns/doap#> .
@prefix mexperf:  <http://mex.aksw.org/mex-perf#> .
@prefix mexalgo:  <http://mex.aksw.org/mex-algo#> .
@prefix dcat:    <http://www.w3.org/ns/dcat#> .
@prefix prov:    <http://www.w3.org/ns/prov#> .
@prefix foaf:    <http://xmlns.com/foaf/0.1/> .
@prefix dc:      <http://purl.org/dc/elements/1.1/> .

this:application
      a       prov:Organization , mexcore:ApplicationContext , prov:Person , prov:Agent ;
      dct:dateCopyrighted "2015.06.30 19:27:12 CEST" ;
      foaf:givenName "D Esteves" ;
      foaf:mbox "esteves@informatik.uni-leipzig.de" .

this:experiment-header
      a       prov:Entity , mexcore:Experiment ;
      dct:date "2015.06.30 19:27:12 CEST" ;
      dct:description "my first MEX experiment" ;
      dct:identifier "E001" ;
      dct:title "my first experiment" ;
      prov:wasAttributedTo
              this:application .

this:model
      a       prov:Entity , mexcore:Model ;
      dct:date "Tue Jun 30 19:27:12 CEST 2015" ;
      dct:description "model generated by LibSVM" ;
      dct:identifier "svm20150322" .

this:context
      a       prov:Entity , mexcore:RecomenderSystems ;
      prov:wasAttributedTo
              this:application .

this:sampling
      a       prov:Entity , mexcore:CrossValidation ;
      mexcore:folds "10" ;
      mexcore:sequential "true" ;
      mexcore:testSize "0.2" ;
      mexcore:trainSize "0.8" .

this:organization
      a       prov:Organization , prov:Agent ;
      foaf:givenName "Leipzig University" .

this:hardware
      a       mexcore:HardwareConfiguration , prov:Entity ;
      mexcore:cache "Cache 3MB" ;
      mexcore:cpu "Intel Core i7" ;
      mexcore:hd "SSD" ;
      mexcore:memory "16GB" ;
      doap:os "ubuntu" .

this:configuration1
      a       mexcore:ExperimentConfiguration , prov:Activity ;
      dct:description "analyzing 10-fold cross validation for SVM" ;
      dct:identifier "E001S001" ;
      prov:used this:model , this:sampling , this:hardware .
